{
    "name": "mridhulka/laravel-oxford-dictionaries-api",
    "description": "Laravel Oxford Dictionaries API Integration.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mridhul",
            "email": "mridhulka@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Mridhulka\\LaravelOxfordDictionariesApi\\": "src/",
            "Mridhulka\\LaravelOxfordDictionariesApi\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mridhulka\\LaravelOxfordDictionariesApi\\LaravelOxfordDictionariesApiServiceProvider"
            ],
            "aliases": {
                "OxfordApi": "Mridhulka\\LaravelOxfordDictionariesApi\\Facades"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^7.22",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.4"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
